/* -- MERAKI DEV -- */ 


/* HOMEPAGE */
/* -------------------- */ 

.flow-homepage{
    overflow: hidden;
}

.merriweather-font .custom-font,
.merriweather-font .h1,
.merriweather-font .h2,
.merriweather-font .h3,
.merriweather-font .h4,
.merriweather-font .h5,
.merriweather-font .h6,
.merriweather-font .header-font,
.merriweather-font h1,
.merriweather-font h2,
.merriweather-font h3,
.merriweather-font h4,
.merriweather-font h5,
.merriweather-font h6,
a.site-nav__link,
.footer-header,
.footer-article,
.logo-bar__item p,
#shopify-section-template--collection label.sidebar-title,
.tags-title-filter, .wetheme-dropdown label[for="tags"]{
    font-family: 'Merriweather', serif;
}

/* ANNOUNCEBAR */

.announcement-bar #sharing li a{
    padding: 0 5px;
}

.announcement-bar #sm-icons li{
    margin-bottom: 0;
}

.announcement-bar__message p{
    letter-spacing: 0;
    font-weight: 500;
    font-family: 'Montserrat';
}

/* HEADER */

.top-links .top-links--icon-links li a{
    padding: 10px 7px;
}

.site-nav .icon-dropdown svg{
    stroke: #e2c265;
    width: 12px;
}

.log-in-button{
    font-style: italic;
}


/* COMMON STYLES */

.title-section{
    font-weight: bold;
    font-size: 30px;
    margin-bottom: 50px;
}
.section.section_pp-image--text-icons .image_text_icons_wrap .subtitle-section-content {
    margin: 0 auto;
    font-weight: 700;
    font-size: 16px;
    color: #6B6B6B;
}
.icon_title{
    margin-bottom:10px;
}

.grid-view-item-image {
    border: 1px solid lightgray;
}

.homepage-product-grid-wrapper {
    align-items: flex-start;
}

.product-grid--title{
    max-width: 250px;
    margin: auto;
}

.product-grid--price .money{
    color: black;
    font-weight: bold;
}

.quick-add-button{
    font-weight: bold;
    letter-spacing: 0px;
    font-size: 12px;
}

.sitewide--title-wrapper{
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}


/* SLIDESHOW */
.homepage-slider-container .swiper-pagination{
    left: 0;
    right: 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.homepage-slider-container .swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet:last-of-type {
    margin-right: 0;
}

.homepage-slider-container .homepage-featured-content-box-inner .featured-row__subtext p{
    font-family: 'Merriweather', serif;
}

.homepage-slider-container h2.homepage-sections--title,
.homepage-slider-container .homepage-sections--pretitle {
    line-height: 1em;
    margin-bottom: 0;
}

.slideshow-wrapper--captionVertical-top{
    top: 10%;
    bottom: inherit;
    transform: none;
}

.slideshow-wrapper--captionVertical-center{
    top: 50%;
    bottom: inherit;
    transform: translateY(-50%);
}

.slideshow-wrapper--captionVertical-bottom{
    bottom: 10%;
    top: inherit;
    transform: none;
}


/* WHO WE ARE */
.logo-section{
    background-size: cover;
    background-repeat: no-repeat;
    position: relative;
}
.logo-bar{
    margin: auto;
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    text-align: center;
}
  .logo-bar__item {
      display: inline-block;
      vertical-align: middle;
      margin: 0 15px 15px 15px;
  }

  @media screen and (max-width: 768px) {
      .logo-bar__item {
          margin: 0 7.5px 15px 7.5px;
      }
  }

  .logo-bar__item img {
      max-width: 100%;
  }

.image-conector{
    position: absolute;
    z-index: 99;
    display: none;
    max-width: 350px;
}

@media screen and (min-width: 1300px) {
    .image-conector{
        display: block;
    }
}


.image-conector.left{
    left: -200px;
}

.image-conector.right{
    right: -250px;
}

.image-conector.bottom{
    top: 50%;
}

.image-conector.top{
    bottom: 50%;
}

.logo-bar__item p{
    margin-top: 10px;
}


/* INGREDIENTS */

.border-btn{
    background: none;
    border: 1px solid;
}

/* ABOUT US */
.homepage-content--image.overlapping-text img{
    max-width: inherit !important;
    object-fit: cover;
}

.homepage-featured-box-btn{
    letter-spacing: 0;
    font-weight: 600;
}

/* PRODUCT PAGE */

body .product-details-product-title,
body #ProductPrice{
    font-family: {{ settings.type_base_font.family }}, {{ settings.type_base_font.fallback_families }};
    color: {{ settings.homepage-sections-accent-primary-color }} !important;
}

.product-details-product-title{
    font-weight: bold;
}

.swiper-pagination-bullet.swiper-pagination-bullet-active, .swiper-pagination-bullet:hover{
    background: {{ settings.homepage-sections-accent-primary-color }};  
}

.featured-product .product-details-wrapper{
    height: calc(100% - 37px);
}

.featured-product #product-box .product-description.rte{
    height: 80px;
    overflow: hidden;
    text-overflow: ellipsis;
}
.featured-product #product-box .product-description.rte h2,
.featured-product #product-box .product-description.rte h3,
.featured-product #product-box .product-description.rte h4,
.featured-product #product-box .product-description.rte .video-wrapper,
.featured-product #product-box .product-description.rte img{
    display: none !important;
}

@media screen and (max-width: 767px){
    .product-details-wrapper {
        padding: 30px;
    }
}


#AddToCartForm{
    display: flex;
    flex-direction: column;
}

#AddToCartForm .rc-container-wrapper.rc_container_wrapper{
    order: 1;
}

#AddToCartForm .rc_widget__option__selector input[type=checkbox], #AddToCartForm .rc_widget__option__selector input[type=radio]{
    margin: 0 5px 0 0;
}

#AddToCartForm .paymentButtonsWrapper{
    order: 2;
}

/* NEWSLETTER SECTION */

.shopify-section .newsletter--section{
    padding: 70px 0;
}

.newsletter--section--grid_inner.newsletter_title h3{
    font-weight: bold;
}

.footer-section--newsletter-wrapper .newsletter--section #newsletter-wrapper #newsletter_text{
    width: 100%;
    border-color: #e5e5e5;
    margin-left: 0;
}

.footer-section--newsletter-wrapper .newsletter--section #newsletter-wrapper{
    position: relative;
}

.footer-section--newsletter-wrapper .newsletter--section #newsletter_submit{
    position: absolute;
    right: 10px;
    bottom: 5px;
}



@media screen and (min-width: 1024) {
    .grid__item.large--one-half.medium--one-half.small--one-whole.newsletter--section--grid.vertical--align {
        width: 45%;
    }

    .footer-section--newsletter-wrapper .newsletter--section .newsletter--section--grid_inner.newsletter_title{
        width: fit-content;
        margin-right: 30px;
    }
    
    .footer-section--newsletter-wrapper .newsletter--section .newsletter--section--grid_inner.newsletter_form_section{
        max-width: 500px;
    }
}



/* SIGN SECTION */
@media screen and (min-width: 1300px) {
    .feature-row--no-overlapping .homepage-indiv-section-wrapper{
        width: 80%;
        margin: 40px auto;
    }
    
    .feature-row--no-overlapping .grid__item.homepage-content--image{
        width: 55%;
    }
    
    .feature-row--no-overlapping .grid__item.feature_row__image_wrapper{
        width: 45%;
    }
}


.homepage-featured-content-box-inner .sign {
    font-family: 'Dancing Script', cursive;
    font-size: 25px;
    line-height: 1em;
}

.feature-row--no-overlapping .grid__item.homepage-content--image *{
    height: 100%;
}

.feature-row--no-overlapping .grid__item.homepage-content--image img{
    object-fit: cover;
}

.featured-row__author{
    font-weight: bold;
}

/* TEXT BANNER SECTION */

.homepage-sections-wrapper--accent .text-advert-section .grid__item{
    border-left: none;
}

.text-advert-section--header.h5{
    font-family: {{ settings.type_base_font.family }}, {{ settings.type_base_font.fallback_families }};
}


/* TESTIMONIAL SECTION */

.testimonial--section {
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
}
.testimonial--single-item {
    background: white;
    margin: 10px 5px;
    max-width: 400px;
    padding: 10px;
    position: relative;
}

.testimonial-triangle{
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 0 20px 15px 20px;
    border-color: transparent transparent #ffffff transparent;
    position: absolute;
    top: -10px;
    left: 50%;
    transform: translateX(-50%);
}

@media screen and (min-width: 1024px) {
    .footer-grid{
        display: grid;
        grid-template-columns: repeat(2, 1fr);
    }    
}


.shop--info{
    display: flex;
    font-weight: bold;
    word-break: break-all;
}

.shop--info svg{
    margin-right: 15px;
    height: 20px;
    min-width: 15px!important;
}

.footer-article, .footer-article a, .footer-content a,
.social-media-footer .shop--info {
    font-family: 'Merriweather', serif;
    font-size: 12px;
}

.line--footer{
    width: 100%;
    height: 20px;
}


.site-footer .social-media-footer ul li a {
    color: {{ settings.color_primary_button_background }};
    padding: 11px;
}

.social-media-footer ul li .fa-2x {
    font-size: 20px;
}


.content-footer img{
    width: 180px;
    margin-bottom: 10px;
}

.manual_width .links-footer{
    display: flex;
    margin: auto;
    justify-content: center;
    flex-wrap: wrap;
}

.links-footer .menu--footer{
    margin-left: 30px;
    margin-right: 30px;
}

@media only screen and (max-width: 480px){
    .links-footer .menu--footer{
        padding-top: 30px;
        border-top: 1px solid var(--color-footer-border);
        margin-top: 30px;
    }

    .footer-header {
        padding-top: 0;
        border-top: none;
    }

    .manual_width .links-footer{
        justify-content: center;
    }

    h6.shop--info{
        justify-content: center;
    }
}

@media screen and (min-width: 481px) and (max-width: 767px) {
    .manual_width .links-footer{
        justify-content: left;
    }
    .links-footer .menu--footer{
        margin-left: 10px;
        margin-right: 10px;
    }    
}



@media screen and (min-width: 1024px) {
    ul.footer-content.footer-grid {
        min-width: 250px;
    } 
}


.menu-footer-2 .footer-header{
    border-top: none;
}
/* COLLECTION */
.collection-sidebar__description img{
    display: none !important;
}

.collection-page--feature-image {
    position: relative;
}

.collection-page--feature-image .page--title{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: #fff;
}

.product-grid--price .sale-color{
    color: red;
}


.swiper-button-disabled {
    background: var(--border-color) !important;
}

.collection-sidebar-filter label.sidebar-title, .collection-sidebar-filter-mobile label.sidebar-title, .collection-sidebar-sort label.sidebar-title,
.tags-title-filter, .wetheme-dropdown label[for="tags"]{
    color: {{ settings.color_primary_button_background }};
    font-size: 20px;
    font-weight: 300;
}

.collection-sidebar-filter .clearall{
    color: {{ settings.color_primary_button_background }};
}


.collection-sidebar-filter label.sidebar-title {
    border-bottom: 1px solid lightgray;
    padding-bottom: 20px;
    margin-bottom: 10px;
    max-width: 200px;
}
.collection-sidebar-filter .clearall, .collection-sidebar-filter-mobile .clearall{
    position: relative;
}

/* PRODUCT PAGE */

#shopify-section-template--product {
    margin-bottom: 50px;
}

#AddToCart, .shopify-product-form .shopify-payment-button__button--unbranded{
    text-transform: uppercase;
    font-weight: bold;
}

.template-product .site-header,
.template-product .mobile-nav-bar-wrapper{
background: #f4f4f4;
}



.social-sharing .sharing-button__icon svg {
    width: 2em;
    height: 2em;
}

.social-sharing a {
    padding: 5px;
}

.social-sharing a:first-of-type{
    margin-left: -5px;
}

.product-page--sale-badge{
    width: fit-content;
    position: absolute;
