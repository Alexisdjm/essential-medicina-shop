}

#ComparePrice{
    color: {{ settings.sale_badge_color }};
}

@media only screen and (min-width: 481px){
    .product-medias__with-thumbnails.product-medias__with-thumbnails--below .product-medias__thumbnails {
        align-items: center;
    }
    
}

.trust-badge-container div{
    margin: 15px 0;
}

/* REVIEWS JUDGEME */
.judgeme-stars{
    margin: 10px 0;
}

#shopify-section-template--product .judgeme-stars{
    margin-bottom: 10px;
    margin-top: 0;
}



/* METAFIELDS */

.pp-image--text{
    display: flex;
    flex-wrap: wrap;
    margin: 15px auto 50px;
    align-items: center;
    justify-content: center;
}

.pp--text--container{
    text-align: center;
}

.pp--image--container img{
    width: 100%;
}

@media screen and (min-width: 1024px) {
    .pp-image--text .pp--image--container,
    .pp-image--text .pp--text--container{
        width: 50%;
    }
    
    .pp--text--container{
        padding: 0 40px 0;
    }  
}

@media screen and (max-width: 1023px) {
    .pp--text--container{
        padding: 10px;
    } 

    .pp-image--text .pp--image--container,
    .pp-image--text .pp--text--container{
        width: 100%;
    }

    .pp-image--text .pp--image--container{
        order: -1;
    }
}

.metafields-title{
    color: {{ settings.color_metafields_title }};
    margin-bottom: 20px;
}

.metafields-description{
    color: {{ settings.color_metafields_description }};
    white-space: break-spaces;
}

.pp--text-icons--container{
    display: flex;
    width: 100%;
    align-items: center;
    margin-top: 30px;
}

.pp--text-icons--container img{
    max-height: 60px;
    margin-right: 30px;
}

.pp--text-icons--container h5{
    text-align: left;
    color: {{ settings.color_primary_button_background }};
    margin-bottom: 0;
}

@media screen and (max-width: 1023px) {
    .pp--text-icons--container{
        flex-wrap: wrap;
    }
    .pp--text-icons--container img{
        margin: auto;
    }
    .pp--text-icons--container h5{
        margin: 10px auto 0;
        text-align: center;
    }
}

.pp--image--container.divided-half-image {
    display: flex;
}

.pp--image--container.divided-half-image img{
    width: 48%;
    margin: 1%;
}



@media screen and (min-width: 1024px) {

.pp--text--container.pp--left{
    text-align: left;
}

.pp--text--container.pp--usedto .descriptions{
    display: flex;
}

.pp--text--container.pp--usedto .descriptions .metafields-description{
    width: 48%;
    margin-right: 2%;
}
}


